# Supabase
SUPABASE_URL=https://<PROJECT>.supabase.co
SUPABASE_JWKS_URL=https://<PROJECT>.supabase.co/auth/v1/jwks
SUPABASE_AUDIENCE=authenticated     # padrão (ajuste se mudou)
SUPABASE_ISSUER=https://<PROJECT>.supabase.co/auth/v1

# Postgres (Pooler é recomendado)
DATABASE_URL=postgresql://postgres:<PASSWORD>@db.<REF>.supabase.co:6543/postgres

# Criptografia
APP_CRED_KEY=<chave-mestre-super-forte>

# API Key (rotas internas, ex.: criação/atualização de credenciais)
INTERNAL_API_KEY=<api-key-forte>

# CORS (origens permitidas p/ Angular)
CORS_ORIGINS=http://localhost:4200,https://app.seu-dominio.com